{
  "name": "pirulug-pma-theme",
  "version": "0.0.1",
  "description": "A modern dark theme for phpMyAdmin",
  "author": "Pirulug",
  "license": "MIT",
  "scripts": {
    "build": "sass scss:css",
    "build:compile": "sass scss:css --style=compressed",
    "build:compile-direct": "sass --style=compressed scss:F:\\xampp\\phpMyAdmin\\themes\\blueberry\\css",
    "build:compile-jq": "sass --style=compressed scss\\jquery-ui.scss:jquery\\jquery-ui.css",
    "build:compile-direct-jq": "sass --style=compressed scss\\jquery-ui.scss:F:\\xampp\\phpMyAdmin\\themes\\blueberry\\jquery\\jquery-ui.css",
    "watch": "sass --watch --style=compressed scss:css",
    "watch:direct": "sass --watch --style=compressed scss:F:\\xampp\\phpMyAdmin\\themes\\blueberry\\css",
    "watch:jq": "sass --watch --style=compressed scss\\jquery-ui.scss:jquery\\jquery-ui.css",
    "watch:direct-jq": "sass --watch --style=compressed scss\\jquery-ui.scss:F:\\xampp\\phpMyAdmin\\themes\\blueberry\\jquery\\jquery-ui.css"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "sass": "^1.80.2"
  }
}
